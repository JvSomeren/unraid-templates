<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>spyfall</Name>
  <Description></Description>
  <Registry>https://hub.docker.com/r/jvsomeren/spyfall</Registry>
  <GitHub>https://github.com/JvSomeren/spyfall</GitHub>
  <Repository>jvsomeren/spyfall</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3000</HostPort>
        <ContainerPort>3000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>METEOR_SETTINGS</Name>
      <Value>"$(cat /spyfall/settings.json)"</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/spyfall/</HostDir>
      <ContainerDir>/spyfall</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <Version></Version>
  <Icon>https://raw.githubusercontent.com/JvSomeren/unraid-templates/master/icons/spyfall.png</Icon>
  <Beta>False</Beta>
  <Category>Network:Other</Category>
  <Date>2019-12-13</Date>
</Container>